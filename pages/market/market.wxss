/* 广播消息窗 */

.loading {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background-color: #fff;
  z-index: 100;
}
.radio-window text {
  float: left;
  font-size: 28rpx;
  color: #000;
  line-height: 50rpx;
  margin-left: 20rpx;
  opacity: 1;
}


.radio-window {
  position: fixed;
  z-index: 10;
  width: 378rpx;
  height: 50rpx;
  left: 20rpx;
  top: 290rpx;
  border-radius: 25rpx;
}

.radio-window image {
  float: left;
  width: 44rpx;
  height: 44rpx;
  margin-left: 5rpx;
  margin-top: 3rpx;
  background-color: #fff;
  border-radius: 50%;
}

