}

.adphotos .foot4 view image {
  width: 100%;
  height: 100%;
}

/* 底部商品列表 */

.footgoodslist {
  width: 100%;
  padding-top: 20rpx;
  box-sizing: border-box;
  background-color: #f7f7f7;
}

.footgoodslist .footgoods {
  float: left;
  display: flex;
  flex-direction: column;
  width: 370rpx;
  height: 540rpx;
  font-size: 28rpx;
  text-align: left;
  /* border-right: 1px solid #e8e8e8;
  border-bottom: 1px solid #e8e8e8; */
