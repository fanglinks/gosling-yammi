}

.footgoodslist .footgoods:nth-child(even) {
  margin-right: 0rpx;
}

.footgoodslist .footgoods image {
  width: 370rpx;
  height: 350rpx;
  /* margin-top: 40rpx;
  margin-left: 12rpx; */
  margin-bottom: 20rpx;
}

.footgoodslist .footgoods .text1 {
  display: block;
  width: 100%;
  height: 76rpx;
  font-size: 26rpx;
  color: #333;
  font-weight: 500;
  padding: 0 10rpx;
  box-sizing: border-box;
}

.footgoodslist .footgoods .text2 {
  text-align: right;
  font-size: 23rpx;
  color: #999;
  margin-top: 20rpx;
  margin-right: 30rpx;
}

.footgoodslist .footgoods .text3 {
  text-align: right;
  font-size: 23rpx;
  color: #fc4d64;
  margin-right: 30rpx;

}

.footgoodslist .footgoods .text4 {
  float: left;
  text-align: left;
  font-size: 23rpx;
  color: #999;
  margin-left: 20rpx;
}


