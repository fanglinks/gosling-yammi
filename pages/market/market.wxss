}
.pingtuan .maohao {
  color: #fc4d64
}

.pingtuan .products {
  width: 100%;
  height: 504rpx;
  background-color: #f7f7f7;
  overflow:hidden;
  white-space:nowrap;
}

.pingtuan .products scroll-view {
  width: 100%;
  height: 504rpx;
   padding-top: 10rpx;
    box-sizing: border-box;
}

.pingtuan .products .product {
  display: inline-block;
  width: 255rpx;
  height: 484rpx;
  text-align: left;
  margin-left: 10rpx;
 background-color: #fff;
}

.pingtuan .products .product image {
  width: 255rpx;
  height: 300rpx;
}

.pingtuan .products .product .text1 {
  display: block;
  height: 78rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 78rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.pingtuan .products .product .text2 {
  display: block;
  font-size: 24rpx;
  color: #333;
}

.pingtuan .products .product .text3 {
  display: block;
  font-size: 24rpx;
  color: #fc4d64;
  padding-left: 20rpx;
  /* margin-bottom: 10rpx; */
  box-sizing: border-box;
}

.pingtuan .products .product .text4 {
  position: relative;
  font-size: 24rpx;
  color: #999;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

