/* 2.0店铺商品样式 */
.goodsLists {
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.goodsLists .goodsList {
  width: 710rpx;
  height: 266rpx;
  padding: 20rpx 0;
  border-bottom: 1px solid #eeeeee;
  box-sizing: border-box;
}

.goodsLists .goodsList image {
  float: left;
  width: 227rpx;
  height: 227rpx;
}

.goodsLists .goodsList .rightList {
  float:left;
  margin-left: 20rpx;
  width: 463rpx;
  overflow: hidden;
}

.goodsLists .goodsList .rightList view {
  overflow: hidden;
}

.goodsLists .goodsList .rightList .titlelist {
  height: 92rpx;
  font-size: 30rpx;
  line-height: 46rpx;
  margin-bottom: 45rpx;
}

.goodsLists .goodsList .rightList .pricelist {
  font-size: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-left: 2rpx;
}

.goodsLists .goodsList .rightList .vippricelist {
  font-size: 35rpx;
  color: #fc4d64;
  height: 60rpx;
  line-height: 55rpx;
}

.goodsLists .goodsList .rightList .vippricelist text {
  display: inline-block;
  font-size: 24rpx;
  line-height: 50rpx;
}

.goodsLists .goodsList .rightList .vippricelist .floatright {
  float: right;
  font-size: 24rpx;
  color: #999;
  line-height: 55rpx;
}

