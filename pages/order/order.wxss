/* 物流信息 */
.wuliuinfo {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #fc4d64;
  opacity: 1;
}

.wuliuinfo scroll-view {
  position: absolute;
  width: 600rpx;
  height: 800rpx;
  margin-left: 75rpx;
  margin-top: 100rpx;
  background-color: #fff;
}

.wuliuinfo .list {
  font-size: 24rpx;
  color: #333;
}

.wuliuinfo .list .left {
  font-size: 30rpx;
  padding: 20rpx;
  box-sizing: border-box;
}

.wuliuinfo .list .right {
  color: #999;
  padding: 20rpx;
  box-sizing: border-box;
}

.wuliuinfo .tuichu {
  font-size: 60rpx;
  color: #fff;
  position: absolute;
  left: 50%;
  margin-left: -30rpx;
  top: 900rpx;
}

