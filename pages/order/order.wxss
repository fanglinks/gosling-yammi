/* pages/order/order.wxss */

page {
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
  padding-top: 70rpx;
  box-sizing: border-box;
}

.header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  flex-direction: row;
  background-color: #fff;
}

.header view {
  position: relative;
  float: left;
  font-size: 30rpx;
  width: 25%;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
}

.header view text {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  width: 28rpx;
  height: 28rpx;
  font-size: 18rpx;
  text-align: center;
  line-height: 28rpx;
  font-weight: 500;
  border: 1px solid #333;
  border-radius: 50%;
}

.header view text.red_color {
  color: #fff;
  border-color: #fc4d64;
  background-color: #fc4d64;
}

