<!--pages/shouquan/shouquan.wxml-->
<view class="container">
  <image src="{{fail?'/img/shouquan/fail.png':'/img/shouquan/shouquan.png'}}"></image>
  <view class="title">{{fail?'登录失败':'快捷登录'}}</view>
  <!-- <view class="comment">为给您带来更好的体验，我们需要您的授权作为快捷登录，我们不会获取您的隐私信息</view> -->
  <button type="primary" open-type="getUserInfo" bindgetuserinfo="onGotUserInfo">{{fail?'重新登录':'微信一键登录'}}</button>  
</view>