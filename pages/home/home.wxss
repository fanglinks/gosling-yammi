/* 优质推荐样式 */
.scroll_NEW {
  width: 100%;
  height: 270rpx;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}

.scroll_NEW .NEW {
  display: flex;
  width: 100%;
  height: 100%;
  white-space: nowrap;
}

.scroll_NEW .NEW .NEW_goods {
  display: inline-block;
  width: 216rpx;
  height: 270rpx;
  margin-right: 30rpx;
}

.scroll_NEW .NEW .NEW_goods:nth-child(1) {
  margin-left: 20rpx;
}

.scroll_NEW .NEW .NEW_goods .img_here {
  width: 100%;
  height: 210rpx;
  border-radius: 8rpx;
  overflow: hidden;
}

.scroll_NEW .NEW .NEW_goods image {
  width: 100%;
  height: 210rpx;
  border-radius: 8rpx;
}

.scroll_NEW .NEW .NEW_goods text {
  display: block;
  font-size: 30rpx;
  color: #333;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 20rpx;
}

